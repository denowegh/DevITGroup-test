{
	"name": "devitgroup-test",
	"version": "1.0.0",
	"main": "server/index.ts",
	"scripts": {
		"start": "node built/index.js",
		"watch": "nodemon --watch server -e ts -x \"ts-node -r dotenv/config -P server/tsconfig.json server/index.ts\"",
		"build:server": "tsc -p server",
		"build:client": "webpack --config server/webpack/webpack.prod.ts",
		"build": "npm run build:server && npm run build:client",
		"check:server": "tsc -p server --noEmit",
		"check:client": "tsc -p client --noEmit",
		"check": "npm run check:server && npm run check:client"
	},
	"dependencies": {
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
		"axios": "^1.5.0",
		"babel-loader": "^9.1.3",
		"body-parser": "^1.20.2",
		"css-loader": "^6.8.1",
		"css-minimizer-webpack-plugin": "^5.0.1",
		"express": "^4.18.2",
		"express-promise-router": "^4.1.1",
		"express-rate-limit": "^7.2.0",
		"html-webpack-plugin": "^5.5.3",
		"mini-css-extract-plugin": "^2.7.6",
		"morgan": "^1.10.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-refresh": "^0.14.0",
		"sass-loader": "^13.3.2",
		"terser-webpack-plugin": "^5.3.9",
		"webpack": "^5.88.2",
		"webpack-dev-middleware": "^6.1.1",
		"webpack-hot-middleware": "^2.25.4",
		"webpack-merge": "^5.9.0"
	},
	"devDependencies": {
		"@babel/core": "^7.22.15",
		"@babel/preset-env": "^7.22.15",
		"@babel/preset-react": "^7.22.15",
		"@babel/preset-typescript": "^7.22.15",
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.5",
		"@types/react": "^18.2.21",
		"@types/react-dom": "^18.2.7",
		"@types/webpack-hot-middleware": "^2.25.6",
		"dotenv": "^16.3.1",
		"nodemon": "^3.0.1",
		"postcss-loader": "^7.3.3",
		"postcss-preset-env": "^9.1.3",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2",
		"webpack-cli": "^5.1.4"
	}
}
